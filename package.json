{
	"name": "@paxport/model-paxshop",
	"version": "0.0.98",
	"description": "Data model for the Paxport Shop.",
	"author": "Paxport Aktiebolag",
	"license": "MIT",
	"repository": "https://github.com/paxport-shop/model-paxshop",
	"bugs": {
		"url": "https://github.com/paxport-shop/model-paxshop/issues"
	},
	"homepage": "https://paxport.com",
	"private": false,
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"type": "module",
	"git": {
		"tagName": "v${version}"
	},
	"github": {
		"release": true
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsconfig": "tsconfig.test.json"
			}
		},
		"transform": {
			"^.+\\.(j|t)sx?$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!(cryptly|authly|isoly|gracely|cloudly-http|cloudly-router|cloudly-rest)/.*)"
		],
		"testEnvironment": "node",
		"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
		"testPathIgnorePatterns": [
			"node_modules/",
			"dist/"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/node_modules/**",
			"!**/dist/**"
		],
		"preset": "ts-jest",
		"testMatch": null
	},
	"scripts": {
		"lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
		"fix": "eslint '**/*.{ts,tsx}' --fix",
		"build": "tsc -p .",
		"test": "jest",
		"prepare": "npm run build",
		"clean": "rimraf dist node_modules coverage"
	},
	"devDependencies": {
		"@types/jest": "^28.1.7",
		"@typescript-eslint/eslint-plugin": "5.45.1",
		"@typescript-eslint/parser": "5.45.1",
		"dotenv": "^16.0.3",
		"eslint": "^8.29.0",
		"eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20221021",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"jest": "^28.1.3",
		"prettierx": "github:utily/prettierx#utily-20221021",
		"rimraf": "^3.0.2",
		"ts-jest": "^28.0.8",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"cloudly-http": "^0.0.53",
		"cloudly-rest": "^0.0.18",
		"cryptly": "^3.0.2",
		"gracely": "^2.0.3",
		"isoly": "^2.0.14",
		"langly": "2.0.3",
		"isomorphic-fetch": "^3.0.0"
	}
}
